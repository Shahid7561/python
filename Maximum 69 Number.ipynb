    "def maximum69Number(num):\n",
    "    res = [n for n in str(num)]\n",
    "    for r in range(len(res)):\n",
    "        if res[r] == '6':\n",
    "            res[r] = '9'\n",
    "            break\n",
    "    return int(\"\".join(res))\n",
    "\n",
    "print(maximum69Number(num))"
